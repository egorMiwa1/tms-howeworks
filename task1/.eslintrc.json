{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "quotes": ["error", "double"],
        "linebreak-style": ["error", "windows"],
        "max-len": ["error", { "code": 150 }],
        "spaced-comment": ["error", "always"],
        "prefer-const": "off"
    }
}